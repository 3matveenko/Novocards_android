<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:bottom="1dp"
		android:left="-10dp"
		android:right="-10dp"
		android:top="-3dp">

		<shape android:shape="rectangle">
			<stroke
				android:width="1dp"
				android:color="#3B888498" />

			<solid android:color="#00FFFFFF" />

			<padding
				android:bottom="5dp"
				android:left="10dp"
				android:right="10dp"
				android:top="5dp" />
		</shape>
	</item>
</layer-list>